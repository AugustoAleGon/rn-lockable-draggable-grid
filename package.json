{
  "name": "@tailwindapp/rn-lockable-draggable-grid",
  "keywords": [
    "drag",
    "drop",
    "sortable",
    "lockable",
    "grid",
    "drag and drop"
  ],
  "version": "1.0.3",
  "description": "A draggable grid for react native",
  "main": "dist/src/index.js",
  "types": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tailwind/rn-lockable-draggable-grid.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w --outDir dist",
    "test": "jest"
  },
  "prepare": "yarn build",
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).ts?(x)"
    ]
  },
  "author": "codycrowok",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/react": "^16.0.0",
    "@types/react-native": "*",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "jest-react-native": "^18.0.0",
    "pre-commit": "^1.2.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.0.3"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-native": "*"
  },
  "bugs": {
    "url": "https://github.com/tailwind/rn-lockable-draggable-grid/issues"
  },
  "homepage": "https://github.com/tailwind/rn-lockable-draggable-grid#readme",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "files": [
    "dist/*"
  ]
}
